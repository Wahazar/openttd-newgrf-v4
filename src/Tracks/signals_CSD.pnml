    //light signals
    //TT         VV        SS
    (0 << 16) | (0 << 8) | 0 : LS00_cs_signals_block_red;
    (0 << 16) | (0 << 8) | 1 : LS01_cs_signals_block_green;
    (0 << 16) | (0 << 8) | 2 : LS02_cs_signals_block_yellow;
    (0 << 16) | (0 << 8) | 3 : LS03_cs_signals_block_yellowblink;
    (1 << 16) | (0 << 8) | 0 : LS10_cs_signals_entry_red;
    (1 << 16) | (0 << 8) | 1 : LS11_cs_signals_entry_green;
    (2 << 16) | (0 << 8) | 0 : LS20_cs_signals_exit_red;
    (2 << 16) | (0 << 8) | 1 : LS21_cs_signals_exit_green;
    (3 << 16) | (0 << 8) | 0 : LS30_cs_signals_combo_red;
    (3 << 16) | (0 << 8) | 1 : LS31_cs_signals_combo_green;
    (4 << 16) | (0 << 8) | 0 : LS40_cs_signals_path2w_red;
    (4 << 16) | (0 << 8) | 1 : LS41_cs_signals_path2w_green;
    (4 << 16) | (0 << 8) | 2 : LS42_cs_signals_path2w_yellow;
    (4 << 16) | (0 << 8) | 3 : LS43_cs_signals_path2w_yellowblink;
    (5 << 16) | (0 << 8) | 0 : LS50_cs_signals_path1w_red;
    (5 << 16) | (0 << 8) | 1 : LS51_cs_signals_path1w_green;
    (5 << 16) | (0 << 8) | 2 : LS52_cs_signals_path1w_yellow_yellow;
    (5 << 16) | (0 << 8) | 3 : LS53_cs_signals_path1w_green_yellow;
    (6 << 16) | (0 << 8) | 0 : LS60_cs_signals_program_red;
    (6 << 16) | (0 << 8) | 1 : LS61_cs_signals_program_green;
//semaphores
    (0 << 16) | (1 << 8) | 0 : LS00_cs_sema_block_red;
    (0 << 16) | (1 << 8) | 1 : LS01_cs_sema_block_green;
    (0 << 16) | (1 << 8) | 2 : LS01_cs_sema_block_green;
    (0 << 16) | (1 << 8) | 3 : LS01_cs_sema_block_green;
    (1 << 16) | (1 << 8) | 0 : LS10_cs_sema_entry_red;
    (1 << 16) | (1 << 8) | 1 : LS11_cs_sema_entry_green;
    (2 << 16) | (1 << 8) | 0 : LS20_cs_sema_exit_red;
    (2 << 16) | (1 << 8) | 1 : LS21_cs_sema_exit_green;
    (3 << 16) | (1 << 8) | 0 : LS30_cs_sema_combo_red;
    (3 << 16) | (1 << 8) | 1 : LS31_cs_sema_combo_green;
    (4 << 16) | (1 << 8) | 0 : LS40_cs_sema_path2w_red;
    (4 << 16) | (1 << 8) | 1 : LS41_cs_sema_path2w_green;
    (4 << 16) | (1 << 8) | 2 : LS42_cs_sema_path2w_green_yellow;
    (4 << 16) | (1 << 8) | 3 : LS42_cs_sema_path2w_green_yellow;
    (5 << 16) | (1 << 8) | 0 : LS50_cs_sema_path1w_red;
    (5 << 16) | (1 << 8) | 1 : LS51_cs_sema_path1w_green;
    (5 << 16) | (1 << 8) | 2 : LS52_cs_sema_path1w_green_yellow;
    (5 << 16) | (1 << 8) | 3 : LS51_cs_sema_path1w_green;
    (6 << 16) | (1 << 8) | 0 : LS60_cs_sema_program_red;
    (6 << 16) | (1 << 8) | 1 : LS61_cs_sema_program_green;

    CB_FAILED;
}




